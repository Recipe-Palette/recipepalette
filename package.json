{
  "name": "recipe-palette",
  "private": true,
  "description": "The best way to track your favorite recipes",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --watch"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.3",
    "@emotion/core": "^10.0.22",
    "@emotion/styled": "^10.0.22",
    "@mdx-js/react": "^1.5.1",
    "@reach/router": "^1.2.1",
    "@theme-ui/color": "^0.2.49",
    "@theme-ui/components": "^0.2.50",
    "apollo-boost": "^0.4.4",
    "axios": "^0.19.1",
    "cleave.js": "^1.5.3",
    "date-fns": "^2.9.0",
    "emotion-theming": "^10.0.19",
    "formik": "^2.1.1",
    "fraction.js": "^4.0.12",
    "gatsby": "^2.16.1",
    "gatsby-background-image": "^0.9.11",
    "gatsby-image": "^2.2.41",
    "gatsby-plugin-create-client-paths": "^2.1.21",
    "gatsby-plugin-manifest": "^2.2.37",
    "gatsby-plugin-netlify": "^2.1.31",
    "gatsby-plugin-offline": "^3.0.34",
    "gatsby-plugin-sharp": "^2.3.5",
    "gatsby-plugin-theme-ui": "^0.2.43",
    "gatsby-source-filesystem": "^2.1.40",
    "gatsby-transformer-sharp": "^2.3.6",
    "graphql-tag": "^2.10.1",
    "intersection-observer": "^0.7.0",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.15",
    "query-string": "^6.10.1",
    "react": "^16.10.2",
    "react-content-loader": "^5.0.1",
    "react-dom": "^16.10.2",
    "react-dropzone": "^10.2.1",
    "react-icons": "^3.7.0",
    "react-toast-notifications": "^2.4.0",
    "react-use-auth": "^0.5.1",
    "theme-ui": "^0.2.44",
    "typescript": "^3.7.5",
    "yup": "^0.28.0"
  },
  "devDependencies": {
    "@testing-library/cypress": "^5.0.2",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "babel-jest": "^24.9.0",
    "babel-preset-gatsby": "^0.2.23",
    "cypress": "^3.6.1",
    "eslint": "^6.7.2",
    "eslint-config-kentcdodds": "^14.6.1",
    "eslint-plugin-cypress": "^2.7.0",
    "husky": "^3.1.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "lint-staged": "^9.5.0",
    "prettier": "^1.18.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-hello-world"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint"
    ],
    "*.+(js|jsx|json|yml|yaml|css|less|scss|ts|tsx|md|graphql|mdx)": [
      "prettier --write",
      "jest --findRelatedTests",
      "git add"
    ]
  }
}
